<?xml version="1.0" encoding="UTF-8"?>
<structure
        moduleType="bankCard"
        type="bankCard"
        description="Standart bank card"
        version="1"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="bankCard.xsd"
        xmlns:inheritance="http://www.bssys.com.MBSClient.XMLInheritance.html"
        >

    <title>
        <caption default="true"/>
        <color value="808080"/>
    </title>

    <infoPanels inheritance:list="id">
        <panel id="News"/>
        <panel id="CurrencyRates"/>
        <panel id="ATM"/>
    </infoPanels>

    <authentication inheritance:list="id">
        <method id="password"/>
    </authentication>

</structure>